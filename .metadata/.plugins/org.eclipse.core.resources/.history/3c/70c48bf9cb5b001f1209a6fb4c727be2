package animal;

public class TestaAnimais {
	public static void main(String[] args) {
		//criando um cachorro
		Cachorro dog1 = new Cachorro();
		dog1.nome = " Rex ";
		dog1.raca = "Labrador";
		dog1.latir(100);
		dog1.comer();
		dog1.dormir();
		dog1.info();
		
		Cachorro dog2 = new Cachorro ();
		dog2.nome = " Snow ";
		dog2.raca = "husky";
		dog2.latir(20);
		dog2.dormir();
		dog2.info();
	}
	
	
	
}
